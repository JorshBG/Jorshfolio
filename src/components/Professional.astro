---
import professionalExperience from "../ts/export/professionalExperience";
import StepTimeLine from "./StepTimeLine.astro";
---

<div class="card mx-5 mt-5 mb-5 tab-content hidden" id="professional-experience">

  <ol class="relative border-s border-gray-200 dark:border-gray-700">
    {
      professionalExperience.map((exp) => {
        return <StepTimeLine experience={exp} />;
      })
    }
  </ol>
</div>
